{
  "name": "@accestech/loghelper",
  "version": "0.0.2",
  "description": "Log helper",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "author": "AccesTech",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "dev": "rm -rf dist && rollup -c -w",
    "lint": "prettier --write .",
    "release": "standard-version --sign",
    "test": "yarn run test:unit && yarn run test:functional",
    "test:unit": "jest test/unit",
    "test:coverage": "jest test/unit --coverage",
    "test:functional": "jest test/functional",
    "test:keys": "jest test/functional -t 'Unupgraded Command Successful'",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,md,yml}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/accestech/loghelper.git"
  },
  "standard-version": {
    "scripts": {
      "prerelease": "yarn test && yarn build"
    }
  },
  "sideEffects": false,
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/jest": "^27.0.1",
    "dotenv": "^10.0.0",
    "husky": "^7.0.0",
    "jest": "^26.0.1",
    "lint-staged": "^11.0.0",
    "prettier": "2.3.2",
    "rollup": "^2.45.2",
    "standard-version": "^9.1.0",
    "ts-jest": "^26.1.0",
    "ts-mockito": "^2.3.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  }
}
